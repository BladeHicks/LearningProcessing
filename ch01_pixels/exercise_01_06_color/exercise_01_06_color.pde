// Exercise 1-6: What color will each
// of the following lines of code generate?
background(255);
noStroke();

// Dark Green
fill(0, 100, 0);
ellipse(20, 40, 16, 16);

// Grey
fill(100);
ellipse( 40, 40, 16, 16);

// Blue stroke
stroke(0, 0, 200);
ellipse(60, 40, 16, 16);

// White stroke
stroke(255);
ellipse(20, 60, 16, 16);

// Yellow stroke
stroke(255, 255, 0);
noFill();
ellipse(40, 60, 16, 16);

// Cyan stroke
stroke(0, 255, 255);
noFill();
ellipse(60, 60, 16, 16);

// Muted Red
noStroke();
fill(200, 50, 50);
ellipse(20, 80, 16, 16);
